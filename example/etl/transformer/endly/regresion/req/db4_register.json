{
  "Datastore": "db4",
  "Config": {
    "DriverName": "aerospike",
    "Descriptor": "tcp([host]:3000)/[namespace]",
    "Parameters": {
      "dbname": "db4",
      "namespace": "db4",
      "host": "$serviceHost",
      "port": "3000",
      "dateFormat": "yyyy-MM-dd hh:mm:ss",
      "keyColumnName": "id"
    }
  }
}