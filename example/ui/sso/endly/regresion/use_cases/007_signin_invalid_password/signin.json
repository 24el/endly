[

  {
    "Selector": {
      "Value": "#email"
    },
    "Calls": [
      {
        "Method": "Clear"
      },
      {
        "Method": "SendKeys",
        "Parameters": [
          "$email"
        ]
      }
    ]
  },

  {
    "Selector": {
      "Value": "#password"
    },
    "Calls": [
      {
        "Method": "Clear"
      },
      {
        "Method": "SendKeys",
        "Parameters": [
          "$password"
        ]
      }
    ]
  },
   {
    "Selector": {
      "Value": "#submit"
    },
    "Calls": [
      {
        "Method": "Click",
        "Wait": {
          "SleepTimeMs": 100
        }
      }
    ]
  },
  {
    "Selector": {
      "By":"xpath",
      "Value": "//SMALL[preceding-sibling::INPUT[@id='email']]",
      "Key":"email"
    },
    "Calls": [
      {
        "Method": "Text"
      }
    ]
  }
]