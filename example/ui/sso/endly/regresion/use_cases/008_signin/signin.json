[

  {
    "Selector": {
      "Value": "#email"
    },
    "Calls": [
      {
        "Method": "Clear"
      },
      {
        "Method": "SendKeys",
        "Parameters": [
          "$email"
        ]
      }
    ]
  },

  {
    "Selector": {
      "Value": "#password"
    },
    "Calls": [
      {
        "Method": "Clear"
      },
      {
        "Method": "SendKeys",
        "Parameters": [
          "$password"
        ]
      }
    ]
  },
   {
    "Selector": {
      "Value": "#submit"
    },
    "Calls": [
      {
        "Method": "Click",
        "Wait": {
          "SleepInMs": 1000
        }
      }
    ]
  },
  {
    "Calls": [
      {
        "Method": "CurrentURL",
        "Wait": {
          "Repeat": 10,
          "SleepInMs": 2000,
          "ExitCriteria": "$value:/viant/"
        }
      }]
  }
]