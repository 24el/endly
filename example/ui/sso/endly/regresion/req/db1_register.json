{
  "Datastore": "db1",
  "Config": {
    "DriverName": "aerospike",
    "Descriptor": "tcp([host]:3000)/[namespace]",
    "Parameters": {
      "dbname": "db1",
      "namespace": "db1",
      "host": "$targetHost",
      "port": "3000",
      "dateFormat": "yyyy-MM-dd hh:mm:ss",
      "keyColumnName": "email"
    }
  }
}