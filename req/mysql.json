{
  "Name": "dockerized_mysql",
  "Tasks":"$task",
  "Params": {
    "stopSystemMysql": true,
    "url": "scp://$targetHost/",
    "credential": "$targetCredential",
    "configUrl": "config/my.cnf",
    "mysqlCredential":"$mysqlCredential",
    "configUrlCredential": "$configUrlCredential",
    "serviceInstanceName": "$instance"
  }
}