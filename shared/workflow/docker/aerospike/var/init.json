[
  {
    "Name": "name",
    "When": "$in.params.name:!/$/ && $in.params.name",
    "From": "params.name",
    "Else": "aerospike_endly"
  },
  {
    "Name": "target",
    "When": "$in.params.target:!/$/ && $in.params.target",
    "From": "params.target",
    "Else": {
      "URL": "ssh://127.0.0.1/",
      "Credentials": "localhost"
    }
  },
  {
    "Name": "env",
    "When": "$in.params.env:!/$/ && $in.params.env",
    "From": "params.env",
    "Else": {}
  },
  {
    "Name": "serviceTarget",
    "From": "params.serviceTarget",
    "When": "$in.params.serviceTarget:!/$/ && $in.params.serviceTarget",
    "Else": {
      "URL": "ssh://127.0.0.1/",
      "Credentials": "localhost"
    }
  },
  {
    "Name": "version",
    "When": "$in.params.latest:!/$/ && $in.params.latest",
    "From": "params.version",
    "Else": "latest"
  },
  {
    "Name": "image",
    "When": "$in.params.image:!/$/ && $in.params.image",
    "From": "params.image",
    "Else": "aerospike/aerospike-server:$version"
  },
  {
    "Name": "config",
    "From": "params.config"
  },
  {
    "Name": "hasConfig",
    "When": "$out.config:!/$/ && $out.config",
    "Value": "1",
    "Else": "0"
  },
  {
    "Name": "serviceConfig",
    "Value": "/tmp/${name}.conf"
  }
]