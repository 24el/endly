[
  {
    "Name": "containerName",
    "Value": "go_build",
    "From": "params.containerName",
    "Required": true
  },
  {
    "Name": "dockerImage",
    "Value": "rastasheep/ubuntu-sshd:14.04",
    "From": "params.dockerImage",
    "Required": true
  },
  {
    "Name": "origin",
    "When":"$params.origin:!/$/ && $params.origin",
    "Value": "$AsMap($params.origin)",
    "Else": ""
  },
  {
    "Name": "upload",
    "Value": [],
    "From": "params.upload",
    "Required": true
  },
  {
    "Name": "download",
    "Value": [],
    "From": "params.download",
    "Required": true
  },
  {
    "Name": "app",
    "From": "params.app",
    "Required": true
  },
  {
    "Name": "commands",
    "From": "params.commands",
    "Required": true
  },

  {
    "Name": "target",
    "When": "$in.params.target:!/$/ && $in.params.target",
    "From": "params.target",
    "Else": {
      "URL": "ssh://127.0.0.1/",
      "Credentials": "localhost"
    }
  },
  {
    "Name": "buildTarget",
    "Value": {
      "URL": "scp://127.0.0.1:7722/${app}",
      "Credentials": "mem://github.com/viant/endly/workflow/docker/build/secret/build.json"
    }
  }
]