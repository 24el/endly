[
  {
    "Name": "name",
    "When": "$in.params.name:!/$/ && $in.params.name",
    "From": "params.name",
    "Else": "mongo_endly"
  },
  {
    "Name": "env",
    "When": "$in.params.env:!/$/ && $in.params.env",
    "From": "params.env",
    "Else": {}
  },
  {
    "Name": "serviceTarget",
    "From": "params.serviceTarget",
    "When": "$in.params.serviceTarget:!/$/ && $in.params.serviceTarget",
    "Else": {
      "URL": "ssh://127.0.0.1/",
      "Credentials": "localhost"
    }
  },
  {
    "Name": "version",
    "When": "$in.params.latest:!/$/ && $in.params.latest",
    "From": "params.version",
    "Else": "latest"
  },
  {
    "Name": "image",
    "When": "$in.params.image:!/$/ && $in.params.image",
    "From": "params.image",
    "Else": "mongo:$version"
  }
]