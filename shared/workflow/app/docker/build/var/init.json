[
  {
    "Name": "containerName",
    "Value": "go_build",
    "From": "params.containerName",
    "Required": true
  },
  {
    "Name": "dockerImage",
    "Value": "rastasheep/ubuntu-sshd:14.04",
    "From": "params.dockerImage",
    "Required": true
  },
  {
    "Name": "app",
    "From": "params.app",
    "Required": true
  },
  {
    "Name": "version",
    "From": "params.version",
    "Required": true
  },
  {
    "Name": "reuseContainer",
    "Required": true,
    "From": "params.reuseContainer",
    "Value": "true"
  },
  {
    "Name": "replace",
    "Required": true,
    "From": "params.replace",
    "Value": {}
  },
  {
    "Name": "expand",
    "Required": true,
    "From": "params.expand",
    "Value": true
  },
  {
    "Name": "upload",
    "Required": true,
    "From": "params.upload",
    "Value": []
  },
  {
    "Name": "download",
    "Value": [],
    "From": "params.download",
    "Required": true
  },
  {
    "Name": "superUser",
    "Required": true,
    "Params": "params.superUser",
    "Value": false
  },
  {
    "Name": "commands",
    "Required": true,
    "From": "params.commands"
  },
  {
    "Name":"useRegistry",
    "Required": true,
    "From":"params.useRegistry",
    "Value":true
  },
  {
    "Name": "buildPath",
    "Required": true,
    "From": "params.buildPath",
    "Value": "/tmp/${app}/build/"
  },
  {
    "Name": "releasePath",
    "Required": true,
    "From": "params.buildRoot",
    "Value": "/tmp/${app}/release/"
  },
  {
    "Name": "dockerfile",
    "Required": true,
    "From": "params.dockerfile",
    "Value": {
      "URL": "config/Dockerfile"
    }
  },
  {
    "Name": "registry",
    "From": "params.registry"
  },
  {
    "Name": "registryUsername",
    "Required": true,
    "From": "params.registryUsername",
    "Value": "endly"
  },
  {
    "Name": "registryCredentials",
    "Required": true,
    "From": "params.registryCredentials",
    "Value": "mem://github.com/viant/endly/workflow/app/docker/secret/registry.json"
  },
  {
    "Name": "sourceTag",
    "From": "params.sourceTag",
    "Required": true,
    "Value": {
      "Username": "$registryUsername",
      "Image": "$app",
      "Version": "$version"
    }
  },
  {
    "Name": "targetTag",
    "From": "params.targetTag",
    "Required": true,
    "Value": {
      "Registry": "$registry",
      "Image": "$app",
      "Version": "$version"
    }
  },
  {
    "Name": "target",
    "From": "params.target",
    "Value": {
      "URL": "ssh://127.0.0.1/",
      "Credentials": "localhost"
    }
  },
  {
    "Name": "buildTarget",
    "Value": {
      "URL": "scp://127.0.0.1:7722/${buildPath}",
      "Credentials": "mem://github.com/viant/endly/workflow/app/docker/secret/root.json"
    }
  }
]