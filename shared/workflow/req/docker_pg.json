{
  "Env": {
    "POSTGRES_PASSWORD": "***pg***",
    "POSTGRES_USER": "###pg###"
  },
  "Target": "$dockerTarget",
  "Secrets": {
    "###pg###": "$mysqlCredential",
    "***pg***": "$mysqlCredential"
  },
  "Name": "$serviceInstanceName",
  "Image": "postgresql:$pgVersion",
  "Mount": {
    "$configFile": "/etc/postgresql/postgresql.conf"
  },
  "Ports": {
    "5432": "5432"
  }
}