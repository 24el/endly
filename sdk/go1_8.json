{
  "Meta": {
    "Sdk": "go",
    "SdkVersion": "1.8",
    "Deployments": [
      {
        "OsTarget": {
          "System": "linux"
        },

        "Deploy": {
          "AppName": "go",
          "Transfer": {
            "Source": {
                "URL": "https://redirector.gvt1.com/edgedl/go/go1.8.5.linux-amd64.tar.gz"
            },
            "Target": {
              "Name": "go",
              "Version": "1.8",
              "URL": "scp://${targetHost}/tmp/sdk/go1.8.5.linux-amd64.tar.gz",
              "Credential": "${targetHostCredential}"
            }
          },
          "VersionCheck": {
            "Options": {
              "SystemPaths": [
                "/usr/local/go-1_8/bin"
              ]
            },
            "Executions": [
              {
                "Command": "go version",
                "Extraction": [
                  {
                    "Key": "Version",
                    "RegExpr": "go(\\d\\.\\d)\\..+"
                  }
                ]
              }
            ]
          },
          "Command": {
            "Options": {
              "Directory": "/tmp/sdk",
              "TimeoutMs":120000
            },
            "Executions": [
              {
                "Command": "tar xvzf go1.8.5.linux-amd64.tar.gz",
                "Error": [
                  "Error"
                ]
                },
              {
                "Command": "rm -rf  /usr/local/go-1_8"
              },
              {
                "Command": "mv /tmp/sdk/go /usr/local/go-1_8"
              }
            ]
          }
        }
      },
      {
        "OsTarget": {
          "System": "darwin"
        },
        "Deploy": {
          "AppName": "go",
          "Transfer": {
            "Source": {
              "URL": "https://redirector.gvt1.com/edgedl/go/go1.8.5.darwin-amd64.tar.gz"
            },
            "Target": {
              "Name": "go",
              "Version": "1.8",
              "URL": "scp://${targetHost}/tmp/sdk/go1.8.5.darwin-amd64.tar.gz",
              "Credential": "${targetHostCredential}"
            }
          },
          "VersionCheck": {
            "Options": {
              "SystemPaths": [
                "/usr/local/go-1_8/bin"
              ]
            },
            "Executions": [
              {
                "Command": "go version",
                "Extraction": [
                  {
                    "Key": "Version",
                    "RegExpr": "go(\\d\\.\\d)\\..+"
                  }
                ]
              }
            ]
          },
          "Command": {
            "Options": {
              "Directory": "/tmp/sdk",
              "TimeoutMs":120000
            },
            "Executions": [
              {
                "Command": "tar xvzf go1.8.5.darwin-amd64.tar.gz",
                "Error": [
                  "Error"
                ]
              },
              {
                "Command": "rm -rf  /usr/local/go-1_8"
              },
              {
                "Command": "mv /tmp/sdk/go /usr/local/go-1_8"

              }
            ]
          }
        }
      }
    ]
  }
}