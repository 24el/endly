[
  {
    "Name": "sdk",
    "Required": true,
    "From": "params.sdk",
    "Value": "$sdk"
  },
  {
    "Name": "GOPATH",
    "Required": true,
    "From": "params.GOPATH",
    "Value": "${env.GOPATH}"
  },
  {
    "Name": "app",
    "Required": true,
    "From": "params.app",
    "Value": "$app"
  },
  {
    "Name": "target",
    "Required": true,
    "From": "params.target",
    "Value": {
      "URL": "ssh://127.0.0.1/",
      "Credentials": "localhost"
    }
  },
  {
    "Name": "serviceTarget",
    "Required": true,
    "From": "params.serviceTarget",
    "Value": "$target"
  },
  {
    "Name": "buildTarget",
    "Required": true,
    "From": "params.buildTarget",
    "Value": "$target"
  },
  {
    "Name": "appTarget",
    "Required": true,
    "From": "params.appTarget",
    "Value": "$target"
  },
  {
    "Name": "targetHost",
    "Value": "$Hostname($target.URL)"
  },
  {
    "Name": "appHost",
    "Value": "$Hostname($appTarget.URL)"
  },
  {
    "Name": "appVersion",
    "Value": "0.1"
  },
  {
    "Name": "appPath",
    "Required": true,
    "From": "params.appPath",
    "Value": "/opt/${app}"
  },
  {
    "Name": "seleniumServerPort",
    "Required": true,
    "From": "params.seleniumServerPort",
    "Value": "8077"
  },
  {
    "...":"$credentials"
  }
]