{
  "Name": "go",
  "Sdk":"go",
  "SdkVersion":"$build.sdkVersion",
  "Goals": [
    {
      "Name": "build",
      "Command": {
        "Options": {
          "Directory": "$build.target",
          "TimeoutMs": 720000

        },
        "Executions": [
          {
            "Command": "cd $build.target"
          },
          {
            "Command": "go clean",
            "Errors": [
              "Error","command not found"
            ]
          },
          {
            "Command": "go get -u .",
            "Errors": [
              "Error","command not found"
            ]
          },
          {
            "Command": "go ${build.goal} ${build.args}",
            "Error": ["failed", "error"],
            "Extraction": [
              {
                "Key":"Artifact",
                "RegExpr": "Building jar:[^\/]+(.+)"
              }
            ]
          }
        ]
      }
    }
  ]
}