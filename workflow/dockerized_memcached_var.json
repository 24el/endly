[
  {
    "Name": "serviceInstanceName",
    "Value": "$params.serviceInstanceName"
  },
  {
    "Name": "dockerTarget",
    "Value": {
      "Name": "$params.serviceInstanceName",
      "URL": "$params.url",
      "Credential": "$params.credential"
    }
  },
  {
    "Name": "sourceConfig",
    "Value": {
      "URL": "$params.configUrl",
      "Credential": "$params.configUrlCredential"
    }
  },
  {
    "Name": "serviceInstanceName",
    "Value": "$params.serviceInstanceName"
  },
  {
    "Name": "systemDockerRequest",
    "Value": {
      "Service": "docker",
      "Target": "$dockerTarget"
    }
  },
  {
    "Name": "dockerRequest",
    "Value": {
      "Target": "$dockerTarget"
    }
  },
  {
    "Name": "maxMemory",
    "From": "params.maxMemory",
    "Value": {
      "Target": "512m"
    }
  },
  {
    "Name": "dockerRunRequest",
    "Value": {
      "SysPath": [
        "/usr/local/bin"
      ],
      "Target": "$dockerTarget",
      "Image": "library/memcached:alpine",
      "MappedPort": {
        "11211": "11211"
      },
      "Params": {
        "-m": "$maxMemory"
      }
    }
  }
]