[
  {
    "name": "serviceInstanceName",
    "type": "const",
    "source": "$params.serviceInstanceName"
  },
  {
    "name": "remoteTarget",
    "type": "const",
    "scope": "in",
    "source": {
      "Name": "$params.serviceInstanceName",
      "URL": "$params.url",
      "Credential": "$params.credential"
    }
  },
  {
    "name": "serviceInstanceName",
    "type": "const",
    "source": "$params.serviceInstanceName"
  },
  {
    "name": "configFile",
    "type": "const",
    "source": "/tmp/aerospike${serviceInstanceName}.conf"
  },
  {
    "name": "configTransfer",
    "type": "const",
    "scope": "in",
    "source": {
      "Source": {
        "URL": "$params.configUrl",
        "Credential": "$params.configUrlCredential"
      },
      "Target": {
        "URL": "file://$configFile"
      },
      "Parsable": true
    }
  }
]